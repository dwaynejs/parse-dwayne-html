{
  "name": "transform-dwayne-html",
  "version": "5.3.0",
  "main": "lib/index.js",
  "typings": "index.d.ts",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babylon": "^6.17.4",
    "entities": "^1.1.1",
    "generate-code": "^2.2.2",
    "lines-and-columns": "^1.1.6",
    "lodash": "^4.17.4",
    "source-map": "^0.5.6",
    "transform-dwayne-js-expressions": "^5.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "mocha": "^3.4.2",
    "nyc": "^11.0.2",
    "sourcemap-codec": "^1.3.1"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "test": "nyc mocha test/index.js --reporter dot",
    "prepublish": "npm test && npm run build"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "sourceMap": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.js",
      "node_modules/**/*"
    ],
    "reporter": [
      "html"
    ],
    "cache": true,
    "all": true
  }
}
